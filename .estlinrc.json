{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "ignorePatterns": ["node_modules/", "dist/", "tests/**/*"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": 0,
    "operator-linebreak": 0,
    "consistent-return": 0,
    "arrow-body-style": 0
  }
}